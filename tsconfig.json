{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["jest", "@ionic/core"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "isolatedModules": true,
    "noUnusedLocals": true,
     "noUnusedParameters": true,
    "jsx": "react-jsx"
  },
  "include": ["src/**/*.tsx", "src/**/*.ts", "src/index.jsx"]
}
 